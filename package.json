{
  "name": "nmbr",
  "version": "1.1.1",
  "description": "Isomorphic JavaScript numbers parser and formatter",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "lint": "./node_modules/.bin/tslint \"src/**/*.*\"",
    "test": "npm run lint && ./node_modules/.bin/jest --no-cache",
    "build": "npm run lint && ./node_modules/.bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webschik/nmbr.git"
  },
  "keywords": [
    "JavaScript",
    "number",
    "format",
    "parse",
    "Node.js"
  ],
  "author": "webschik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webschik/nmbr/issues"
  },
  "homepage": "https://github.com/webschik/nmbr#readme",
  "devDependencies": {
    "@types/es6-shim": "^0.31.32",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-jest": "^18.0.0",
    "jest": "^18.1.0",
    "ts-loader": "^2.0.0",
    "tslint": "^4.3.1",
    "tslint-eslint-rules": "^3.2.3",
    "typescript": "^2.1.5",
    "webpack": "^1.14.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "test/coverage",
    "coverageReporters": [
      "cobertura"
    ],
    "testRegex": "(/test/unit/.*|\\.(spec))\\.(ts|js)$",
    "transform": {
      ".*": "<rootDir>/jest/preprocessor.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest/config.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/e2e/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
